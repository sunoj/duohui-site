    include partials/_mixins.pug
    .bg-black-geo.text-white
      .container
        .py-4.flex.items-center
          .flex.items-center
            a(href="/"): img(src='/img/logo-light.png', srcset='/img/logo-light@2x.png 2x', alt='多会')
